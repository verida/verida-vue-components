{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "webpack-env",
      "jest",
    ],
    "paths": {
      "@/*": [
        "/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "/**/*.ts",
    "/**/*.tsx",
    "/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx"
, "tests/unit/account/vda-login.spec.tsss"  ],
  "exclude": [
    "node_modules"
  ]
}
